#!/bin/bash -e
TAG=$1
if [ -z "$TAG" ]
then
    echo "Usage promote [tag]"
    exit 2
fi
TIMESTAMP=`date "+%Y%m%d-%H:%M"`
TAG="sb-$TIMESTAMP-$TAG"
svn copy https://svn.corp.flipkart.com/svn/flipkart_dessert/branches/dev-branches/platform/stable https://svn.corp.flipkart.com/svn/flipkart_dessert/fk-pf-fqservice/sb/$TAG
